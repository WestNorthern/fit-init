<h1>A list of all the workouts!</h1>

<div class="workouts">
	<hr>
	<hr>
	<% @workouts.each do |workout| %>
		<div class="workout">
			<%= workout.title %>
			<br>
			<%= workout.description %>
			<br>
			<%= workout.video_url %>
			<br>
			<%= workout.min_lvl %>
			<br>
			<%= workout.min_reps %>
			<br>
		</div>
			<br>
			<hr>
	<% end %>
	<hr>
	<hr>


</div>
